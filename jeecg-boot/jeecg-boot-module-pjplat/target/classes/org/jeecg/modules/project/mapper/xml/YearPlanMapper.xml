<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.project.mapper.YearPlanMapper">

	<delete id="deleteByMainId" parameterType="java.lang.Long">
		DELETE
		FROM  pjplat.bs_year_plan 
		WHERE
			 yp_prj_id = #{mainId} 
	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.Long" resultType="org.jeecg.modules.project.entity.YearPlan">
		SELECT * 
		FROM  pjplat.bs_year_plan
		WHERE
			 yp_prj_id = #{mainId} 
	</select>

</mapper>
